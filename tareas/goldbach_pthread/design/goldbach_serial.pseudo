procedure goldbach_serial
    init arreglo
    datos = get_entrada_file(.txt)
    verdad = validate(datos)
    if (verdad)
        if es_par(datos)
            do conjetura fuerte
            realizar_sumas(item)
            insert_arreglo(arreglo)
            send_to_entrada(nodo)
        if es_impar
            do conjetura debil
            realizar_sumas(item)
            insert_arreglo(arreglo)
            send_to_entrada(nodo)
        
    else
        ignorar
    print arreglo
    free arreglo(arreglo)
    free nodo(nodo)
end procedure
